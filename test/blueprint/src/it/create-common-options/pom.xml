        <license.verbose>true</license.verbose>
    </properties>

    <build>

        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>license-maven-plugin</artifactId>
                    <version>@pom.version@</version>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

</project>